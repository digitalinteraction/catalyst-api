{
  "name": "@openlab/catalyst-node-api",
  "private": true,
  "version": "1.1.0",
  "description": "A ChowChow API to serve projects over HTTP using JSON",
  "repository": "unplatform/catalyst-node-api",
  "author": "Rob Anderson (https://r0b.io)",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "npx tsc --noEmit",
    "coverage": "jest --coverage",
    "preversion": "npm run lint && npm test -s",
    "postversion": "git push --follow-tags",
    "prettier": "prettier --write '**/*.{js,ts,tsx,json,css,md}'",
    "start": "node -r dotenv/config dist/server.js",
    "test": "jest",
    "dev": "NODE_ENV=development nodemon -w src -e ts -x 'npx ts-node -r dotenv/config' src/server.ts",
    "gen-readme-toc": "npx @robb_j/md-toc -i",
    "release": "standard-version"
  },
  "keywords": [
    "not-equal-catalyst",
    "chowchow",
    "api"
  ],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@openlab/trello-client": "^0.1.1",
    "@robb_j/chowchow": "^0.6.2",
    "@robb_j/chowchow-json-envelope": "^0.2.3",
    "@robb_j/chowchow-logger": "^0.3.1",
    "bufferutil": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "monk": "^6.0.6",
    "redis": "^2.8.0",
    "shuffle-array": "^1.0.1",
    "superstruct": "^0.8.2",
    "utf-8-validate": "^5.0.2",
    "uuid": "^3.3.3",
    "valid-env": "^0.1.3",
    "ws": "^6.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@robb_j/md-toc": "^0.3.1",
    "@types/cors": "^2.8.6",
    "@types/jest": "^24.0.23",
    "@types/node": "^11.9.4",
    "@types/redis": "^2.8.14",
    "@types/shuffle-array": "0.0.28",
    "@types/superstruct": "^0.8.2",
    "@types/uuid": "^3.4.6",
    "@types/ws": "^6.0.4",
    "akita-ws": "^0.1.2",
    "jest": "^24.9.0",
    "lint-staged": "^8.1.4",
    "nodemon": "^1.18.10",
    "prettier": "^1.19.1",
    "standard-version": "^8.0.1",
    "ts-jest": "^23.10.5",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.2",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
    "pre-commit": "lint-staged"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
